{
  "name": "my-project",
  "version": "0.1.0",
  "bin": {
    "my-project": "bin/my-project.js"
  },
  "config": {
    "event": "cnsec2022"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:dev-diff": "EVENTABBR=$npm_package_config_event cdk diff -c config=dev",
    "cdk:stg-diff": "EVENTABBR=$npm_package_config_event cdk diff -c config=stg",
    "cdk:prd-diff": "EVENTABBR=$npm_package_config_event cdk diff -c config=prd",
    "cdk:dev-deploy": "EVENTABBR=$npm_package_config_eventcdk cdk deploy -c config=dev",
    "cdk:stg-deploy": "EVENTABBR=$npm_package_config_eventcdk cdk deploy -c config=stg --require-approval never --all",
    "cdk:prd-deploy": "EVENTABBR=$npm_package_config_eventcdk cdk deploy -c config=prd --require-approval never --all"
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "@types/node": "10.17.27",
    "@types/node-fetch": "2.6.2",
    "@types/prettier": "2.6.0",
    "aws-cdk": "2.32.1",
    "jest": "27.5.1",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.137.0",
    "@aws-sdk/client-ivs": "3.137.0",
    "@aws-sdk/util-dynamodb": "3.137.0",
    "aws-cdk-lib": "2.32.1",
    "constructs": "10.1.49",
    "node-fetch": "3.2.10",
    "source-map-support": "0.5.21"
  }
}
